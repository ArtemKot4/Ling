{
    "name": "ling",
    "displayName": "Ling support",
    "description": "This plugin adds highlighting and analyze to ling format of files.",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "author": "ArtemKot",
    "publisher": "ArtemKot",
    "activationEvents": [
        "onLanguage:ling"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "ling",
                "aliases": [
                    "ling"
                ],
                "extensions": [
                    ".ling"
                ]
            }
        ],
        "grammars": [
            {
                "language": "ling",
                "scopeName": "source.ling",
                "path": "./syntaxes/ling.tmLanguage.json"
            }
        ]
    },
    "icon": "./icon.png",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
        "@types/vscode": "^1.107.0",
        "typescript": "^5.9.3"
    }
}